# 服务器配置
PORT=7070
HOST=0.0.0.0

# 静态资源目录（前端构建产物）
STATIC_DIR=./dist

# 管理员 API Key
API_KEY=sk-my-secret-key

# PostgreSQL 配置
DATABASE_URL=postgresql://user:password@localhost:5432/llmio

# Redis 配置
# 格式: redis://[[username:]password@]host[:port][/database]
# 示例:
#   无密码: redis://localhost:6379
#   有密码: redis://:yourpassword@localhost:6379
#   用户名+密码: redis://user:password@localhost:6379
#   指定数据库: redis://:password@localhost:6379/0
REDIS_URL=redis://localhost:6379
REDIS_PREFIX=llmio:dev
REDIS_DEFAULT_TTL_SECONDS=1800
